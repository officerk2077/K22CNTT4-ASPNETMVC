@model IEnumerable<NtkLesson06CF.Models.NtkCategory>

@{
    ViewBag.Title = "NtkIndex";
}

<h2>NtkIndex</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NtkCategoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NtkCategoryName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.NtkId }) |
            @Html.ActionLink("Details", "Details", new { id=item.NtkId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.NtkId })
        </td>
    </tr>
}

</table>
